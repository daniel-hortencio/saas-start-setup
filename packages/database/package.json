{
  "name": "@repo/database",
  "version": "0.0.0",
  "main": "./dist/index.ts",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    ".",
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf .turbo node_modules dist",
    "dev": "tsup --watch",
    "format": "prisma format",
    "generate": "prisma generate",
    "lint": "eslint ./src --fix",
    "prebuild": "npm run generate",
    "predev": "npm run generate",
    "studio": "prisma studio",
    "db:init": "docker compose up -d && sleep 20 && npx prisma migrate dev && docker compose down",
    "db:start": "docker compose up -d db",
    "db:seed": "tsx src/seed.ts",
    "db:stop": "docker compose down && docker container prune -f",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:dev:create": "npx prisma migrate dev --create-only",
    "db:push": "prisma db push --skip-generate"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^17.0.45",
    "dotenv-cli": "^7.2.1",
    "prisma": "^5.9.1",
    "rimraf": "^3.0.2",
    "tsup": "^5.11.13",
    "tsx": "^3.7.1",
    "typescript": "^4.9.5"
  }
}
